{
  "author": "ysstech",
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "start": "node ./scripts/build.js --watch",
    "build": "node ./scripts/build.js",
    "new": "node ./scripts/new.js",
    "changelog": "lerna-changelog",
