    "prettier": "^1.17.1",
    "puppeteer": "^2.1.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-test-renderer": "^16.8.6",
    "rimraf": "^2.6.3",
    "shelljs": "^0.8.3",
    "slash": "^3.0.0",
    "styled-components": "^4.2.0",
    "stylelint": "^13.2.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "terser": "^4.8.1",
    "through2": "^3.0.1",
    "vinyl-fs": "^3.0.3"
