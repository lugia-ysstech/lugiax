{
  "name": "@lugia/lugiax",
  "version": "1.2.6",
  "dependencies": {
    "@lugia/combine": "^1.0.2",
    "@lugia/lugiax-core": "^1.2.6",
    "@lugia/lugiax-data": "^1.1.5",
    "hoist-non-react-statics": "^3.3.2",
    "immutable": "^3.8.2"
  },
  "peerDependencies": {
    "react": ">16.0.0",
    "react-dom": ">16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lugia-ysstech/lugiax"
  },
  "license": "Apache License 2.0",
  "homepage": "http://lugia.tech",
  "keywords": [
    "lugia",
    "lugiax",
    "react",
    "redux",
    "react-redux"
  ],
  "author": "ysstech",
  "scripts": {
    "build": "lugia-complie lint && lugia-complie build",
    "dist": " npm run build && npm publish",
    "dev": "./tasks/dev.sh",
    "test": "./tasks/test.sh",
    "cov": "lugia-complie cov",
    "start": "node scripts/start.js",
    "flow": "flow"
  },
  "files": [
    "lib",
    "interface"
  ],
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": []
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "testMatch": [
      "<rootDir>/target/test/**/*.{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "main": "lib/index.js",
  "type": "./lib/index.d.ts",
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.2.0"
  },
  "devDependencies": {
    "flow-bin": "^0.77.0",
    "react-dev-utils": "9.1.0",
    "react-test-renderer": "16.4.1"
  }
}
