{
  "name": "@lugia/lugiax-persistence",
  "version": "1.1.1",
  "keywords": [
    "lugia",
    "lugiax",
    "lugiax-persistence"
  ],
  "author": "ysstech",
  "scripts": {
    "build": "lugia-complie lint && lugia-complie build",
    "dist": " npm run build && npm publish",
    "test": "./tasks/test.sh",
    "dev": "./tasks/dev.sh",
    "cov": "lugia-complie cov",
    "start": "node scripts/start.js",
    "flow": "flow"
  },
  "devDependencies": {
    "@lugia/jverify": "^1.0.1",
    "@lugia/lugiax": "^1.2.2",
    "@lugia/react-test-utils": "^1.0.2",
    "file-loader": "1.1.5",
    "flow-bin": "0.74.0"
  },
  "files": [
    "lib",
    "interface"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lugia-ysstech/lugiax"
  },
  "license": "Apache License 2.0",
  "homepage": "http://lugia.tech",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "testMatch": [
      "<rootDir>/target/test/**/*.{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "main": "./lib/index.js",
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.2.0"
  }
}
